<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>

<table>
  <tr>
    <th>Store Id</th>
    <th>Manager staff id</th>
    <th>Address id</th>
    <th>Last update</th>
  </tr>

   {{#each data}}
  <tr>
    <td>{{this.store_id}}</td>
    <td>{{this.manager_staff_id}}</td>
    <td>{{this.address_id}}</td>
    <td>{{this.last_update}}</td>
  </tr>
  {{/each}}
</table>